{
  "presets": ["react-native"],
  "plugins": [
    ["babel-plugin-transform-decorators-legacy"],
    ["babel-plugin-react-native-config", {
        envfile: ".env"
    }],
    ["module-resolver", {
        root: ["./src"],
        extensions: [".js",".tsx"],
        alias: {
          "styled-components": "styled-components/native",
        }
      }
    ]
  ]
}
